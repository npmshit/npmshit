  "bugs": {
    "url": "https://github.com/npmshit/npmshit/issues"
  },
  "homepage": "https://github.com/npmshit/npmshit#readme",
  "devDependencies": {
    "prettier": "^1.13.6",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "commander": "^2.15.1",
    "rd": "^2.0.0"
  }
}
