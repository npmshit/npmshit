{
  "name": "npmshit",
  "version": "0.1.1",
  "description": "自动清理node_modules中无用的文件",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "bin/npmshit.js",
    "dist/cli.js",
    "dist/cli.d.ts",
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "bin": {
    "npmshit": "./bin/cli.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
